{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "The Document schema",
  "description": "The Document are referenced in dB table with this schema.",
  "examples": [
      {
          "id": 1,
          "title": "DÉCRYPTER",
          "url": "./Files/DÉCRYPTER",
          "created_at": "2020-11-11T13:49:18.690Z",
          "modified_at": "2020-11-11T13:49:18.690Z",
          "deleted_at": null
      }
  ],
  "required": [
    "title",
    "url"
  ],
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The id schema",
          "description": "Auto-generated dB id.",
          "default": 0,
          "examples": [
              1
          ]
      },
      "title": {
          "$id": "#/properties/title",
          "type": "string",
          "title": "The title/name of the pdf file in the file path",
          "description": "titles must be unique & not Null in dB.",
          "examples": [
              "DÉCRYPTER"
          ]
      },
      "url": {
          "$id": "#/properties/url",
          "type": "string",
          "title": "The url schema",
          "description": "urls must be unique & not Null in dB.",
          "examples": [
              "DÉCRYPTER"
          ]
      },
      "created_at": {
          "$id": "#/properties/created_at",
          "type": "string",
          "title": "The created_at schema"
      },
      "modified_at": {
          "$id": "#/properties/modified_at",
          "type": "string",
          "title": "The modified_at schema"
      }
  },
  "additionalProperties": false
}