{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "CheckableKeyword",
  "description": "This is a keyword that could be checked by the user and then used in the documents query.",
  "examples": [
      {
          "id": 1,
          "keyword": "Thaï Union",
          "Category_id": 1
      }
  ],
  "required": [
      "keyword",
      "Category_id"
  ],
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The id schema",
          "description": "dB Identifier",
          "examples": [
              1
          ]
      },
      "keyword": {
          "$id": "#/properties/keyword",
          "type": "string",
          "title": "The keyword schema",
          "description": "This is the actual keyword that could be checked by the user.",
          "examples": [
              "Thaï Union"
          ]
      },
      "Category_id": {
          "$id": "#/properties/Category_id",
          "type": "integer",
          "title": "The Category_id schema",
          "description": "This is the dB identifier of the category that this keyword should be listed under.",
          "examples": [
              1
          ]
      },
      "created_at": {
          "$id": "#/properties/created_at",
          "type": "string",
          "title": "The created_at schema"
      },
      "modified_at": {
          "$id": "#/properties/modified_at",
          "type": "string",
          "title": "The modified_at schema"
      }
  },
  "additionalProperties": false
}